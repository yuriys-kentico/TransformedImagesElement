{
  "compilerOptions": {
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "pretty": true,
    "noFallthroughCasesInSwitch": true,
    "keyofStringsOnly": true,
    "jsx": "react",
    "module": "commonjs",
    "target": "es6"
  },
  "exclude": [
    "node_modules"
  ]
}